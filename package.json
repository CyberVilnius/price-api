{
  "name": "men-microservice-skeleton",
  "version": "1.0.0-git-commit-hash",
  "description": "MEN Microservice skeleton",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "__comment__": "Delay is needed to not crash each second time because 9229 busy",
    "__window_specifics__": "If nodemon doesnt pickup changes automatically -L flag might help",
    "dev": "npx nodemon --delay 2 --inspect=0.0.0.0:9229 index.js",
    "docker:production": "npx cross-env NODE_ENV=production npm start",
    "docker:development": "npx cross-env NODE_ENV=development npm run dev",
    "lint": "npx eslint .",
    "test": "npx cross-env NODE_ENV=test mocha \"./{,!(node_modules)/**/}*.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rayvid/men-microservice-skeleton"
  },
  "keywords": [
    "men",
    "mean",
    "mern",
    "microservice"
  ],
  "author": "Rayvid",
  "license": "SEE LICENSE IN JUSTTAKEANDUSEIT",
  "homepage": "https://github.com/Rayvid/men-microservice-skeleton",
  "dependencies": {
    "@sentry/node": "^6.2.3",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.1.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.1",
    "mongoose": "^5.12.1",
    "simple-oauth2": "^4.2.0",
    "swagger-ui-express": "^4.1.6",
    "underscore": "^1.12.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.22.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7"
  }
}
