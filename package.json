  "homepage": "https://github.com/Rayvid/men-microservice-skeleton",
  "dependencies": {
    "@sentry/node": "^5.7.1",
    "body-parser": "^1.19.0",
    "config": "^1.31.0",
    "cross-env": "^5.2.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-winston": "^3.4.0",
    "joi": "^14.3.1",
    "mongoose": "^5.9.4",
    "swagger-ui-express": "^3.0.10",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
